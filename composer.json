{
	"name": "slothsoft/sse",
	"description": "A Farah module for server-sent events.",
	"license": "WTFPL",
	"authors": [
		{
			"name": "Daniel Schulz",
			"email": "info.slothsoft@gmail.com"
		}
	],
	"type": "library",
	"require": {
		"php": "^7.2",
		"psr/http-message": "^1.0",
		"slothsoft/core": "dev-master",
		"slothsoft/farah": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"Slothsoft\\SSE\\": "src/"
		},
		"files": [
			"src/registerModule.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5"
	},
	"keywords": [
		"slothsoft",
		"farah-module",
		"docs"
	],
	"autoload-dev": {
		"classmap": [
			"tests/"
		]
	},
	"classmap-authoritative": false,
	"minimum-stability": "dev",
	"prefer-stable": true
}
