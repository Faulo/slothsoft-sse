{
    "name": "slothsoft/sse",
    "description": "A Farah module for server-sent events.",
    "license": "GPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmai.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.2",
        "psr/http-message": "^1.0",
        "slothsoft/core": "^1.0",
        "slothsoft/farah": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\SSE\\": "src/"
        },
        "files": [
            "src/registerModule.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "keywords": [
        "slothsoft",
        "farah-module",
        "docs"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false,
    "homepage": "http://farah.slothsoft.net/modules/sse"
